@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-warning" id="btn3">Button3</button>

<script>
    $('#btn1').click(function () {
        swal("Merhaba MVC Proje Kampı");
    })
</script>
<script>
    $('#btn2').click(function () {
        swal("Here's the title!", "...and here's the text!");

    })
</script>

<script>
    $('#btn3').click(function () {
        swal("Good job!", "You clicked the button!", "success");

    })
</script>